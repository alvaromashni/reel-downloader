{
  "version": 2,
  "functions": {
    "api/download.py": {
      "runtime": "@vercel/python@4.7.2"
    }
  },
  "routes": [
    {
      "src": "^/api/download$",
      "dest": "api/download.py"
    },
    {
      "src": "^/$",
      "dest": "public/index.html"
    },
    {
      "src": "^/(.*)$",
      "dest": "public/$1"
    }
  ]
}